translate english MENU_CONTAR_SHEPPARD_cc878c39_1:

    # drc "Certamente."
    drc "Certainly."

# game/cenas/dia1/menus/menu_contar_sheppard.rpy:55
translate english MENU_CONTAR_SHEPPARD_2edc0781:

    # shp "Agradeço por tudo, senhor Rightclue."
    shp "Thanks for everything, Mr Rightclue."

